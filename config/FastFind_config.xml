<?xml version="1.0" encoding="utf-8"?>

<fastfind version="V0.5">
<!--
  <logging noconsole=""/>
-->
  <output_filesystem compression="fast"/>

  <filesystem>
    <location shadows="no">%SystemDrive%\</location>

    <ntfs_find path_match="*\windows\system32\*" name="cmd.exe" />

<!--
    <yara block="20M" overlap="2M" timeout="20" source="res:#ruleset.yara"/> 
    <ntfs_find size_le="8M" yara_rule="*"/>
-->
  </filesystem>

  <registry>    
    <location shadows="no">%SystemDrive%\</location>

    <hive name="SOFTWARE">
      <ntfs_find name="SOFTWARE" />

      <registry_find key_path="\Microsoft\Windows\CurrentVersion\Run" value="SecurityHealth" /> 
    </hive>
  </registry>


</fastfind>
